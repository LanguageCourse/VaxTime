{% if constant('VAX_ANALYTICS_KEY') != '' %}
<script async src="https://www.googletagmanager.com/gtag/js?id={{constant('VAX_ANALYTICS_KEY')}}"></script>
<script>
  window.dataLayer = window.dataLayer || [];
  function gtag(){dataLayer.push(arguments)};
  gtag('js', new Date());

  gtag('config', '{{constant('VAX_ANALYTICS_KEY')}}');
</script>
{% endif %}